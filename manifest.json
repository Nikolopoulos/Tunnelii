{
	"manifest_version":1,
	"type":"WebApp",
	"meta":{
		"name":"__Tunelii",
		"version":"1.0.0",
		"minimum-overwolf-version":"0.81.33",
		"author":"Basil Nikolopoulos",
		"icon":"images/appIcons/normal.png",
		"icon_gray":"images/appIcons/grey.png",
		"description":"Demo App"
	},
	"data": {
		"start_window":"MainWindow",
		"windows":{
			"MainWindow":{
					"file":"pages/index.html",
					"transparent":true,
					"resizable":true,
					"size":{"width":500, "height":500},	
					
					"clickthrough":false
				}
		},
		"extra-objects":{
			"eyex": { 
				"file" : "lib/OverwolfTobii.dll", 
				"class" : "Overwolf.Tobii.TobiiHelper" 
			}
		},
	}
}